{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
    
  ],
  "host_permissions": [
    "http://*/*", 
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "http://*/*", 
        "https://*/*"
      ]
    }
  ]
}
